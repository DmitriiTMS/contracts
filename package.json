{
  "name": "@cinema-micro/contracts",
  "version": "1.0.0",
  "description": "Protobuf defenition and generate TypeScript types",
  "author": "dimon",
  "scripts": {
    "generate": "protoc -I ./proto ./proto/*.proto --ts_proto_out=./gen --ts_proto_opt=nestJs=true,package=omit"
  },
  "files": [
    "proto",
    "gen"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/microservices": "^11.1.14",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.11.2"
  }
}
